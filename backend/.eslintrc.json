{
  "extends": "strongloop",
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2020
  },
  "rules": {
    "new-cap": 0,
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "always"
      }
    ]
  },
  "env": {
    "es6": true
  }
}